{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "app/api/webhook/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "WEBHOOK_SECRET": "@webhook_secret",
    "NEXT_PUBLIC_GITHUB_USERNAME": "@github_username"
  }
}